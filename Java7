import java.util.Scanner;

public class ReadLinesUntilEOF {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int lineNumber = 1;

        while (scanner.hasNextLine()) {
            System.out.println(lineNumber++ + " " + scanner.nextLine());
        }

        scanner.close();
    }
}
