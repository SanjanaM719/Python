import java.util.Scanner;
public class Multiples
{
    public static void main(String args[]){
    int N,result,i;
    Scanner sc = new Scanner(System.in);
    
     N = sc.nextInt();
    
    for( i = 1; i <= 10 ; i++ )
    {
        result = N * i;
        System.out.println(N +" x "+ i +" = "+ result);
    }
        
        
    }
 }   
